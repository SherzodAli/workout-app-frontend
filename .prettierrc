{
	"arrowParens": "avoid",
	"useTabs": true,
	"singleQuote": true,
	"jsxSingleQuote": true,
	"tabWidth": 2,
	"semi": false,
	"printWidth": 80,
	"trailingComma": "none",
	"importOrder": [
		"\\.scss$",
		"<THIRD_PARTY_MODULES>",
		"^@",
		"^../(.*)",
		"^./(.*)"
	],
	"importOrderSeparation": true,
	"importOrderSortSpecifiers": true,
	"plugins": ["@trivago/prettier-plugin-sort-imports"]
}
